<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Python</Name>
  <Description>
    Python utilities for docker
  </Description>
  <Registry>https://registry.hub.docker.com/u/virantha/docker-python/</Registry>
  <GitHub>https://github.com/virantha/docker-python</GitHub>
  <Repository>virantha/docker-python</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5122</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>EDGE</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/python</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:22]/</WebUI>
  <Banner>https://www.python.org/static/img/python-logo.png</Banner>
  <Icon>https://lh5.googleusercontent.com/-zhGeNuhyPc0/AAAAAAAAAAI/AAAAAAAAAFo/Zz3N6Grpz8k/photo.jpg?sz=48</Icon>
</Container>
